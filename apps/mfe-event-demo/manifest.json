{
  "$schema": "https://mfe-made-easy.com/schemas/mfe-manifest-v2.schema.json",
  "name": "eventDemo",
  "version": "1.0.0",
  "url": "http://localhost:8080/mfe-event-demo/mfe-event-demo.js",
  "dependencies": {
    "runtime": {},
    "peer": {
      "react": "^18.0.0 || ^19.0.0",
      "react-dom": "^18.0.0 || ^19.0.0"
    }
  },
  "compatibility": {
    "container": ">=1.0.0",
    "frameworks": {
      "react": ">=18.0.0"
    }
  },
  "requirements": {
    "services": [
      {
        "name": "logger",
        "optional": false
      },
      {
        "name": "eventBus",
        "optional": false
      },
      {
        "name": "auth",
        "optional": true
      },
      {
        "name": "modal",
        "optional": true
      },
      {
        "name": "notification",
        "optional": true
      }
    ]
  },
  "metadata": {
    "displayName": "Event Demo MFE",
    "description": "Demonstrates event bus communication between MFEs",
    "icon": "ðŸ“¡"
  },
  "capabilities": {
    "emits": ["event-demo:message", "event-demo:action"],
    "listens": ["app:theme-change", "user:login", "user:logout", "event-demo:external-message"]
  },
  "config": {
    "loading": {
      "timeout": 30000,
      "retries": 3,
      "retryDelay": 1000
    }
  }
}
