{
  "name": "@mfe/example-mfe",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001",
    "build": "tsc && esbuild src/main.tsx --bundle --format=esm --outfile=dist/mfe-example.js --define:process.env.NODE_ENV='\"production\"' --define:global=globalThis",
    "preview": "vite preview",
    "serve:static": "npx http-server dist -p 8080 --cors -c-1",
    "serve": "npx http-server dist -p 3001 --cors -c-1 -o",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\""
  },
  "dependencies": {
    "@mfe/dev-kit": "workspace:*",
    "@mfe/shared": "workspace:*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "esbuild": "^0.25.8",
    "typescript": "^5.3.3",
    "vite": "^7.0.6"
  }
}
