{
  "$schema": "https://mfe-made-easy.com/schemas/mfe-manifest-v2.schema.json",
  "name": "mfe-modal-solidjs",
  "version": "1.0.0",
  "url": "http://localhost:8080/service-demos/modal/mfe-modal-solidjs/mfe-modal-solidjs.js",
  "alternativeUrls": [],
  "dependencies": {
    "runtime": {
      "solid-js": "^1.8.0"
    },
    "peer": {
      "@mfe-toolkit/build": "^0.1.0"
    }
  },
  "compatibility": {
    "container": "^1.0.0",
    "browsers": {
      "chrome": ">=90",
      "firefox": ">=88",
      "safari": ">=14",
      "edge": ">=90"
    },
    "frameworks": {
      "solid-js": "^1.8.0"
    }
  },
  "capabilities": {
    "emits": [
      "modal:test-opened",
      "modal:test-closed"
    ],
    "listens": [],
    "features": [
      "modal-testing",
      "custom-modals",
      "modal-stacking",
      "solid-signals",
      "fine-grained-reactivity",
      "no-virtual-dom"
    ]
  },
  "requirements": {
    "services": [
      {
        "name": "modal",
        "optional": false
      },
      {
        "name": "logger",
        "optional": true
      }
    ]
  },
  "metadata": {
    "displayName": "Solid.js Modal Demo",
    "description": "modal service demonstration MFE built with Solid.js",
    "icon": "ðŸ”·",
    "author": {
      "name": "MFE Toolkit Team"
    },
    "category": "service-demos",
    "tags": [
      "modal",
      "solidjs",
      "service",
      "reactive"
    ]
  },
  "config": {
    "loading": {
      "timeout": 30000,
      "retries": 3,
      "retryDelay": 1000,
      "priority": 5,
      "preload": false,
      "lazy": true
    },
    "runtime": {
      "isolation": "none",
      "keepAlive": false,
      "singleton": true
    },
    "communication": {
      "eventNamespace": "modal-demo"
    }
  }
}