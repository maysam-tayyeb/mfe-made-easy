{
  "$schema": "https://mfe-made-easy.com/schemas/mfe-manifest-v2.schema.json",
  "name": "mfe-modal-react18",
  "version": "1.0.0",
  "url": "http://localhost:8080/service-demos/modal/mfe-modal-react18/mfe-modal-react18.js",
  "alternativeUrls": [],
  "dependencies": {
    "runtime": {
      "react": "^18.0.0",
      "react-dom": "^18.0.0"
    },
    "peer": {
      "@mfe-toolkit/core": "^0.1.0"
    }
  },
  "compatibility": {
    "container": "^1.0.0",
    "browsers": {
      "chrome": ">=90",
      "firefox": ">=88",
      "safari": ">=14",
      "edge": ">=90"
    },
    "frameworks": {
      "react": "^18.0.0"
    }
  },
  "capabilities": {
    "emits": [
      "modal:test-opened",
      "modal:test-closed"
    ],
    "listens": [],
    "features": [
      "modal-testing",
      "custom-modals",
      "modal-stacking",
      "react18-hooks",
      "concurrent-rendering",
      "suspense",
      "automatic-batching"
    ]
  },
  "requirements": {
    "services": [
      {
        "name": "modal",
        "optional": false
      },
      {
        "name": "logger",
        "optional": true
      }
    ]
  },
  "metadata": {
    "displayName": "React 18 Modal Demo",
    "description": "modal service demonstration MFE built with React 18",
    "icon": "⚛️",
    "author": {
      "name": "MFE Toolkit Team"
    },
    "category": "service-demos",
    "tags": [
      "modal",
      "react18",
      "service",
      "concurrent"
    ]
  },
  "config": {
    "loading": {
      "timeout": 30000,
      "retries": 3,
      "retryDelay": 1000,
      "priority": 5,
      "preload": false,
      "lazy": true
    },
    "runtime": {
      "isolation": "none",
      "keepAlive": false,
      "singleton": true
    },
    "communication": {
      "eventNamespace": "modal-demo"
    }
  }
}