{
  "$schema": "https://mfe-made-easy.com/schemas/mfe-manifest-v2.schema.json",
  "name": "mfe-modal-react18",
  "version": "1.0.0",
  "url": "http://localhost:8080/service-demos/general/mfe-modal-react18/mfe-modal-react18.js",
  "alternativeUrls": [],
  "dependencies": {
    "runtime": {
      "react": "^18.2.0 || ^19.0.0",
      "react-dom": "^18.2.0 || ^19.0.0"
    },
    "peer": {
      "@mfe-toolkit/core": "^0.1.0"
    }
  },
  "compatibility": {
    "container": "^1.0.0",
    "browsers": {
      "chrome": ">=90",
      "firefox": ">=88",
      "safari": ">=14",
      "edge": ">=90"
    },
    "frameworks": {
      "react": "^18.2.0 || ^19.0.0"
    }
  },
  "capabilities": {
    "emits": [],
    "listens": [],
    "features": [
      "react18-hooks",
      "concurrent-features"
    ]
  },
  "requirements": {
    "services": [
      {
        "name": "logger",
        "optional": true
      }
    ]
  },
  "metadata": {
    "displayName": "React 18  Demo",
    "description": "general service demonstration MFE built with React 18",
    "icon": "⚛️",
    "author": {
      "name": "MFE Toolkit Team"
    },
    "category": "service-demos",
    "tags": [
      "general",
      "react18",
      "service"
    ]
  },
  "config": {
    "loading": {
      "timeout": 30000,
      "retries": 3,
      "retryDelay": 1000,
      "priority": 5,
      "preload": false,
      "lazy": true
    },
    "runtime": {
      "isolation": "none",
      "keepAlive": false,
      "singleton": true
    },
    "communication": {
      "eventNamespace": "mfe-modal-react18"
    }
  }
}