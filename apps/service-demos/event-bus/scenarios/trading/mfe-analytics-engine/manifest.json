{
  "$schema": "https://mfe-made-easy.com/schemas/mfe-manifest-v2.schema.json",
  "name": "mfe-analytics-engine",
  "version": "1.0.0",
  "url": "http://localhost:8080/service-demos/event-bus/scenarios/trading/mfe-analytics-engine/mfe-analytics-engine.js",
  "alternativeUrls": [
    "https://cdn.example.com/mfe-analytics-engine/1.0.0/main.js"
  ],
  "dependencies": {
    "runtime": {},
    "peer": {
      "@mfe-toolkit/core": "^0.1.0"
    },
    "optional": {}
  },
  "compatibility": {
    "container": "^1.0.0",
    "browsers": {
      "chrome": ">=90",
      "firefox": ">=88",
      "safari": ">=14",
      "edge": ">=90"
    },
    "frameworks": {}
  },
  "capabilities": {
    "emits": [
      "analytics:portfolio-analyzed",
      "analytics:trend-identified",
      "analytics:report-generated"
    ],
    "listens": [
      "market:trade-executed",
      "market:price-update",
      "trading:order-placed",
      "trading:order-filled"
    ],
    "features": [
      "real-time-analytics",
      "portfolio-analysis",
      "trend-detection",
      "performance-metrics"
    ]
  },
  "requirements": {
    "services": [
      {
        "name": "eventBus",
        "optional": false
      },
      {
        "name": "logger",
        "optional": true
      }
    ],
    "permissions": [],
    "features": []
  },
  "metadata": {
    "displayName": "Analytics Engine",
    "description": "Real-time trading analytics processor built with Vanilla TypeScript",
    "icon": "ðŸ“Š",
    "author": {
      "name": "MFE Toolkit Team"
    },
    "category": "service-demos",
    "tags": [
      "analytics",
      "trading",
      "vanilla-typescript",
      "real-time"
    ]
  },
  "config": {
    "loading": {
      "timeout": 30000,
      "retries": 3,
      "retryDelay": 1000,
      "priority": 5,
      "preload": false,
      "lazy": true
    },
    "runtime": {
      "isolation": "none",
      "keepAlive": false,
      "singleton": true
    },
    "communication": {
      "eventNamespace": "analytics"
    }
  },
  "build": {
    "time": "2025-01-14T00:00:00.000Z",
    "env": "development"
  }
}