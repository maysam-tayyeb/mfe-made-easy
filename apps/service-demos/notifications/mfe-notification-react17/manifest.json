{
  "$schema": "https://mfe-toolkit.com/schemas/mfe-manifest-v2.schema.json",
  "name": "mfe-notification-react17",
  "version": "1.0.0",
  "url": "http://localhost:8080/service-demos/notifications/mfe-notification-react17/mfe-notification-react17.js",
  "dependencies": {
    "runtime": {
      "react": "^17.0.2",
      "react-dom": "^17.0.2"
    },
    "peer": {
      "@mfe-toolkit/core": "^0.1.0"
    }
  },
  "compatibility": {
    "container": "^1.0.0",
    "browsers": {
      "chrome": ">=90",
      "firefox": ">=88",
      "safari": ">=14",
      "edge": ">=90"
    }
  },
  "capabilities": {
    "emits": [
      "notification:test"
    ],
    "listens": [],
    "features": [
      "notification-testing",
      "custom-notifications"
    ]
  },
  "requirements": {
    "services": [
      {
        "name": "notification",
        "optional": false
      },
      {
        "name": "logger",
        "optional": true
      }
    ]
  },
  "metadata": {
    "displayName": "Mfe Notification React17",
    "description": "Mfe Notification React17 MFE",
    "icon": "⚛️",
    "author": {
      "name": "MFE Toolkit Team"
    },
    "category": "service-demos",
    "tags": [
      "react",
      "mfe",
      "demo"
    ]
  },
  "config": {
    "loading": {
      "timeout": 30000,
      "retries": 3,
      "retryDelay": 1000,
      "priority": 5,
      "preload": false,
      "lazy": true
    },
    "runtime": {
      "isolation": "none",
      "keepAlive": false,
      "singleton": true
    },
    "communication": {
      "eventNamespace": "notification-demo"
    }
  }
}
