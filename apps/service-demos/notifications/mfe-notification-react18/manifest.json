{
  "$schema": "https://mfe-made-easy.com/schemas/mfe-manifest-v2.schema.json",
  "name": "mfe-notification-react18",
  "version": "1.0.0",
  "url": "http://localhost:8080/service-demos/notifications/mfe-notification-react18/mfe-notification-react18.js",
  "metadata": {
    "displayName": "React 18 Notification Demo",
    "description": "Notification service demo built with React 18",
    "icon": "⚛️",
    "tags": ["react18", "notifications", "demo"]
  },
  "dependencies": {
    "runtime": {},
    "peer": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "compatibility": {
    "container": ">=1.0.0",
    "frameworks": {
      "react": ">=18.2.0 <19.0.0"
    }
  },
  "requirements": {
    "services": [
      { "name": "notification", "optional": false },
      { "name": "logger", "optional": true }
    ]
  },
  "capabilities": {
    "features": ["notification-testing", "react18-hooks", "custom-notifications"],
    "emits": ["notification:test"],
    "listens": []
  }
}