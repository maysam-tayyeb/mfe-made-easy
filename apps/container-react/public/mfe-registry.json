      "requirements": {
        "services": [
          { "name": "eventBus", "optional": false },
          { "name": "notifications", "optional": true }
        ]
      },
      "metadata": {
        "framework": "vue",
        "category": "trading",
        "capabilities": ["trade", "order", "portfolio"]
      }
    },
    {
      "name": "mfe-analytics-engine",
      "url": "http://localhost:8080/event-bus/scenarios/trading/mfe-analytics-engine.js",
      "version": "1.0.0",
      "description": "Real-time market analytics and insights",
